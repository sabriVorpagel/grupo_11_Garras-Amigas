{
  "name": "garrasamigas",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./src/bin/www",
    "server": "nodemon ./src/bin/www",
    "client:start": "cd my-app & npm run start",
    "client:build": "cd my-app & npm run build",
    "dev": "concurrently \"npm run server\" \"npm run client:start\"",
    "create:db": "sequelize db:create && sequelize db:migrate && sequelize db:seed:all",
    "reset:db": "sequelize db:drop && npm run create:db"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "bootstrap": "^5.2.3",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.8",
    "express": "~4.16.1",
    "express-session": "^1.17.3",
    "express-validator": "^6.14.2",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^8.5.1",
    "method-override": "^3.0.0",
    "morgan": "~1.9.1",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^2.3.3",
    "sequelize": "^6.25.2",
    "sequelize-cli": "^6.5.2",
    "sweetalert2": "^11.6.15"
  },
  "devDependencies": {
    "concurrently": "^7.6.0"
  }
}
